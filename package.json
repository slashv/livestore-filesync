{
  "name": "livestore-filesync-monorepo",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@10.14.0",
  "license": "MIT",
  "description": "Framework-agnostic file syncing for LiveStore applications with OPFS support",
  "repository": {
    "type": "git",
    "url": "https://github.com/example/livestore-filesync"
  },
  "scripts": {
    "build": "pnpm -r build",
    "build:packages": "pnpm --filter '@livestore-filesync/*' build",
    "check": "pnpm -r check",
    "lint": "eslint \"**/{src,test,examples,scripts}/**/*.{ts,tsx,mjs}\"",
    "lint-fix": "pnpm lint --fix",
    "test": "pnpm -r test",
    "dev": "pnpm -r --parallel dev",
    "dev:vue": "pnpm build:packages && pnpm --filter livestore-filesync-vue-example dev",
    "dev:react": "pnpm build:packages && pnpm --filter livestore-filesync-react-example dev",
    "changeset-version": "changeset version",
    "changeset-publish": "pnpm build && changeset publish"
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.5.0",
    "@changesets/cli": "^2.27.8",
    "@effect/eslint-plugin": "^0.3.2",
    "@eslint/compat": "1.1.1",
    "@eslint/eslintrc": "3.1.0",
    "@eslint/js": "9.10.0",
    "@typescript-eslint/eslint-plugin": "^8.4.0",
    "@typescript-eslint/parser": "^8.4.0",
    "eslint": "^9.10.0",
    "eslint-import-resolver-typescript": "^3.6.3",
    "eslint-plugin-codegen": "^0.28.0",
    "eslint-plugin-import": "^2.30.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-sort-destructure-keys": "^2.0.0",
    "tsx": "^4.17.0",
    "typescript": "^5.6.2"
  },
  "pnpm": {
    "patchedDependencies": {
      "@changesets/get-github-info@0.6.0": "patches/@changesets__get-github-info@0.6.0.patch"
    }
  }
}
