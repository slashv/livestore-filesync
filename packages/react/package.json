{
  "name": "@livestore-filesync/react",
  "version": "0.0.1",
  "type": "module",
  "license": "MIT",
  "description": "React adapter for livestore-filesync",
  "keywords": [
    "livestore",
    "file-sync",
    "react",
    "opfs",
    "offline-first"
  ],
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsc -b tsconfig.build.json",
    "check": "tsc -b tsconfig.json",
    "dev": "tsc -b tsconfig.build.json --watch"
  },
  "dependencies": {
    "@livestore-filesync/core": "workspace:*"
  },
  "peerDependencies": {
    "@livestore/livestore": "catalog:",
    "@livestore/react": "catalog:",
    "effect": "catalog:",
    "react": ">=18.0.0"
  },
  "devDependencies": {
    "@livestore/livestore": "catalog:",
    "@livestore/react": "catalog:",
    "@types/react": "^19.1.13",
    "effect": "catalog:",
    "react": "^19.1.0",
    "typescript": "^5.6.2"
  }
}
