{
  "name": "@livestore-filesync/adapter-node",
  "version": "0.0.1",
  "type": "module",
  "license": "MIT",
  "description": "Node filesystem adapter for livestore-filesync",
  "keywords": [
    "livestore",
    "file-sync",
    "adapter",
    "node"
  ],
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsc -b tsconfig.build.json",
    "check": "tsc -b tsconfig.json",
    "dev": "tsc -b tsconfig.build.json --watch"
  },
  "dependencies": {
    "@livestore-filesync/core": "workspace:*"
  },
  "peerDependencies": {
    "@effect/platform": "catalog:",
    "@effect/platform-node": "catalog:",
    "effect": "catalog:"
  },
  "devDependencies": {
    "@effect/platform": "catalog:",
    "@effect/platform-node": "catalog:",
    "@types/node": "^22.5.2",
    "effect": "catalog:",
    "typescript": "^5.6.2"
  }
}
