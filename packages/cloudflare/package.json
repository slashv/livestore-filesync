{
  "name": "@livestore-filesync/cloudflare",
  "version": "0.0.1",
  "type": "module",
  "license": "MIT",
  "description": "Cloudflare Workers helper for livestore-filesync file storage",
  "keywords": [
    "livestore",
    "file-sync",
    "cloudflare",
    "r2",
    "workers"
  ],
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsc -b tsconfig.build.json",
    "check": "tsc -b tsconfig.json",
    "dev": "tsc -b tsconfig.build.json --watch"
  },
  "peerDependencies": {
    "@cloudflare/workers-types": ">=4.0.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20251118.0",
    "typescript": "^5.6.2"
  }
}
