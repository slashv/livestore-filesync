{
  "name": "@livestore-filesync/r2",
  "version": "0.5.2",
  "type": "module",
  "license": "MIT",
  "description": "Cloudflare R2 storage adapter for livestore-filesync with Worker utilities",
  "keywords": [
    "cloudflare",
    "workers",
    "r2",
    "storage",
    "livestore"
  ],
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "!dist/src"
  ],
  "scripts": {
    "build": "tsc -b tsconfig.build.json",
    "check": "tsc -b tsconfig.json",
    "dev": "tsc -b tsconfig.build.json --watch",
    "test": "vitest"
  },
  "devDependencies": {
    "typescript": "^5.6.2",
    "vitest": "^3.2.0"
  }
}
