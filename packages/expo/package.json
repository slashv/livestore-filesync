{
  "name": "@livestore-filesync/expo",
  "version": "0.3.1",
  "type": "module",
  "license": "MIT",
  "description": "Expo filesystem and image processing adapters for livestore-filesync",
  "keywords": [
    "livestore",
    "file-sync",
    "expo",
    "react-native",
    "effect",
    "filesystem",
    "image-processing"
  ],
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": ["dist", "!dist/**/*.test.*"],
  "scripts": {
    "build": "tsc -b tsconfig.build.json",
    "check": "tsc -b tsconfig.json",
    "dev": "tsc -b tsconfig.build.json --watch",
    "test": "vitest"
  },
  "peerDependencies": {
    "@effect/platform": "catalog:",
    "@livestore-filesync/core": "workspace:*",
    "effect": "catalog:",
    "expo-crypto": "^15.0.0",
    "expo-file-system": "^19.0.0",
    "expo-image-manipulator": "^13.0.0 || ^14.0.0"
  },
  "peerDependenciesMeta": {
    "expo-crypto": {
      "optional": true
    }
  },
  "devDependencies": {
    "@effect/platform": "catalog:",
    "@livestore-filesync/core": "workspace:*",
    "effect": "catalog:",
    "@livestore/utils": "catalog:",
    "typescript": "^5.6.2",
    "vitest": "^3.2.0"
  }
}
