{
  "name": "@livestore-filesync/s3-signer",
  "version": "0.5.0",
  "type": "module",
  "license": "MIT",
  "description": "S3-compatible signing service for livestore-filesync (Cloudflare Workers)",
  "keywords": [
    "livestore",
    "file-sync",
    "s3",
    "signer",
    "presigned-url",
    "cloudflare",
    "workers"
  ],
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "!dist/src"
  ],
  "scripts": {
    "build": "tsc -b tsconfig.build.json",
    "check": "tsc -b tsconfig.json",
    "dev": "tsc -b tsconfig.build.json --watch",
    "test": "vitest"
  },
  "peerDependencies": {
    "@cloudflare/workers-types": ">=4.0.0"
  },
  "dependencies": {
    "aws4fetch": "^1.0.20"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20251118.0",
    "typescript": "^5.6.2",
    "vitest": "^3.2.0"
  }
}
