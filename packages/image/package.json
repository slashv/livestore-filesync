{
  "name": "@livestore-filesync/image",
  "version": "0.5.1",
  "type": "module",
  "license": "MIT",
  "description": "Image preprocessing and thumbnail generation for livestore-filesync using wasm-vips",
  "keywords": [
    "livestore",
    "file-sync",
    "image",
    "preprocessor",
    "thumbnails",
    "wasm-vips",
    "resize",
    "jpeg",
    "webp",
    "effect"
  ],
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./preprocessor": {
      "types": "./dist/preprocessor/index.d.ts",
      "import": "./dist/preprocessor/index.js"
    },
    "./preprocessor/canvas": {
      "types": "./dist/preprocessor/canvas-preprocessor.d.ts",
      "import": "./dist/preprocessor/canvas-preprocessor.js"
    },
    "./processor": {
      "types": "./dist/processor/index.d.ts",
      "import": "./dist/processor/index.js"
    },
    "./processor/canvas": {
      "types": "./dist/processor/canvas.d.ts",
      "import": "./dist/processor/canvas.js"
    },
    "./processor/vips": {
      "types": "./dist/processor/vips.d.ts",
      "import": "./dist/processor/vips.js"
    },
    "./thumbnails": {
      "types": "./dist/thumbnails/index.d.ts",
      "import": "./dist/thumbnails/index.js"
    },
    "./thumbnails/schema": {
      "types": "./dist/thumbnails/schema/index.d.ts",
      "import": "./dist/thumbnails/schema/index.js"
    },
    "./thumbnails/worker": {
      "types": "./dist/thumbnails/worker.d.ts",
      "import": "./dist/thumbnails/worker.js"
    },
    "./thumbnails/workers/vips.worker": {
      "types": "./dist/thumbnails/workers/vips.worker.d.ts",
      "import": "./dist/thumbnails/workers/vips.worker.js"
    },
    "./thumbnails/workers/canvas.worker": {
      "types": "./dist/thumbnails/workers/canvas.worker.d.ts",
      "import": "./dist/thumbnails/workers/canvas.worker.js"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "!dist/**/*.test.*",
    "!dist/test"
  ],
  "scripts": {
    "build": "tsc -b tsconfig.build.json",
    "check": "tsc -b tsconfig.json",
    "dev": "tsc -b tsconfig.build.json --watch",
    "test": "vitest"
  },
  "dependencies": {
    "@livestore-filesync/core": "workspace:*"
  },
  "devDependencies": {
    "@effect/platform": "catalog:",
    "@effect/vitest": "^0.25.1",
    "@livestore/livestore": "catalog:",
    "@types/node": "^22.5.2",
    "effect": "catalog:",
    "typescript": "^5.6.2",
    "vitest": "^3.2.0",
    "wasm-vips": "^0.0.16"
  },
  "peerDependencies": {
    "@effect/platform": "catalog:",
    "@livestore/livestore": "catalog:",
    "effect": "catalog:",
    "wasm-vips": "^0.0.16"
  },
  "peerDependenciesMeta": {
    "@livestore/livestore": {
      "optional": true
    },
    "wasm-vips": {
      "optional": true
    }
  }
}
