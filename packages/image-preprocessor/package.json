{
  "name": "@livestore-filesync/image-preprocessor",
  "version": "0.3.1",
  "type": "module",
  "license": "MIT",
  "description": "Image preprocessing for livestore-filesync using wasm-vips",
  "keywords": [
    "livestore",
    "file-sync",
    "image",
    "preprocessor",
    "wasm-vips",
    "resize",
    "jpeg",
    "webp"
  ],
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "!dist/**/*.test.*"
  ],
  "scripts": {
    "build": "tsc -b tsconfig.build.json",
    "check": "tsc -b tsconfig.json",
    "dev": "tsc -b tsconfig.build.json --watch",
    "test": "vitest"
  },
  "peerDependencies": {
    "@livestore-filesync/core": "workspace:*",
    "wasm-vips": "^0.0.11"
  },
  "devDependencies": {
    "@livestore-filesync/core": "workspace:*",
    "typescript": "^5.6.2",
    "vitest": "^3.2.0"
  }
}
